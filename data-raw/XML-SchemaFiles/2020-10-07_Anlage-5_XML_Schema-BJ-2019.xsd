	<xsd:group name="c_Haendedesinfektion">
		<xsd:annotation>
			<xsd:documentation>Gruppe, die den Container "Haendedesinfektion" enthält.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Haendedesinfektion">
