	<xsd:simpleType name="format_icd_3_5">
		<xsd:annotation>
			<xsd:documentation>ICD 3 bis 5 stellig / Format: X99[.99]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[a-zA-Z0-9][0-9]{2}(\.[0-9]{1,2})?"/>
		</xsd:restriction>
	</xsd:simpleType>
