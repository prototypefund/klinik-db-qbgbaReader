							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="PQ_ZP_Schluessel" type="enum_Schluessel_PQ_ZP_ohneSonstige">
										<xsd:annotation>
											<xsd:documentation>Schlüssel-Nr. der Fachexpertise.</xsd:documentation>
											<xsd:documentation source="chapter">B-X.11.2</xsd:documentation>
