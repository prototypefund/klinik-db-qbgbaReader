					</xsd:annotation>

					<xsd:selector xpath="Teilnahme_DMP"/>
					<xsd:field xpath="Bezeichnung"/>
				</xsd:unique>

			</xsd:element>
		</xsd:sequence>
	</xsd:group>

	<xsd:group name="c_Sonstige_Verfahren_Externe_QS">
