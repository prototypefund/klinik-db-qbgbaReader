											<xsd:documentation/>
											<xsd:documentation source="chapter">C-4</xsd:documentation>
											<xsd:documentation source="aggregation">vereinigungsmengeSchluessel</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="Referenzbereiche" type="char_max100" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation/>
											<xsd:documentation source="chapter">C-4</xsd:documentation>
											<xsd:documentation source="aggregation">vereinigungsmengeSchluessel</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="Vergleichswerte" type="char_max100" minOccurs="0">
