          </xsd:annotation>
          
          <xsd:selector xpath="Fachexpertise"/>
          <xsd:field xpath="AQ_ZF_Schluessel"/>
        </xsd:unique>
        
      </xsd:element>
    </xsd:sequence>
  </xsd:group>

  <xsd:group name="c_Pflegekraefte">
    <xsd:annotation>
      <xsd:documentation>Gruppe, die den Container "Pflegekraefte" enthaelt.</xsd:documentation>
    </xsd:annotation>
    
    <xsd:sequence>
      <xsd:element name="Pflegekraefte">
        <xsd:annotation>
          <xsd:documentation>Anlage 1: A-11.2, B-X.11.2</xsd:documentation>
        </xsd:annotation>
        
        <xsd:complexType>
          <xsd:sequence>
