        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Anzahl_VK" type="float_0d00_10000d00"/>
            <xsd:group ref="s_Erlaeuterungen_300" minOccurs="0"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
