          <xsd:sequence>
            <xsd:group ref="c_Datensatz">
              <xsd:annotation>
                <xsd:documentation>Informationen zum Datensatz.</xsd:documentation>
              </xsd:annotation>
            </xsd:group>

            <xsd:group ref="c_Software">
