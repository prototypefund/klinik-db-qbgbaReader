      <xsd:documentation>Gruppe, die den Container "Personelle_Ausstattung" enthaelt.</xsd:documentation>
    </xsd:annotation>
    
    <xsd:sequence>
      <xsd:element name="Personelle_Ausstattung">
        <xsd:annotation>
          <xsd:documentation>Anlage 1: B-X.11</xsd:documentation>
        </xsd:annotation>
        
        <xsd:complexType>
          <xsd:sequence>
            
            <xsd:group ref="c_Aerztliches_Personal">
              <xsd:annotation>
                <xsd:documentation>Anlage 1: B-X.11.1</xsd:documentation>
              </xsd:annotation>
            </xsd:group>
            
            <xsd:group ref="c_Aerztliche_Fachexpertisen" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Anlage 1: B-X.11.1</xsd:documentation>
              </xsd:annotation>
            </xsd:group>
            
            <xsd:choice>
              <xsd:annotation>
                <xsd:documentation>Anlage 1: B-X.11.2</xsd:documentation>
              </xsd:annotation>
              
              <xsd:group ref="c_Pflegekraefte"/>
              <xsd:element name="Keine_Pflegekraefte" type="empty"/>
            </xsd:choice>

            <xsd:group ref="c_Pflegerische_Fachexpertisen" minOccurs="0">
