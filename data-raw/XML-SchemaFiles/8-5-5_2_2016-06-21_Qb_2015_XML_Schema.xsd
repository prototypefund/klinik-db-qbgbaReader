  <xsd:simpleType name="qb_date">
    <xsd:annotation>
      <xsd:documentation>Der Datentyp laesst nur die Datumsangaben des Formates "YYYY-MM-DD" zu.</xsd:documentation>
    </xsd:annotation>

    <xsd:restriction base="xsd:date">
      <xsd:pattern value="(.){10}"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="qb_time">
    <xsd:annotation>
      <xsd:documentation>Der Datentyp laesst nur die Uhrzeitangaben des Formates "HH:MM:SS" zu.</xsd:documentation>
    </xsd:annotation>
    
    <xsd:restriction base="xsd:time">
      <xsd:pattern value="(.){8}"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:simpleType name="qb_email">
    <xsd:annotation>
      <xsd:documentation>Datentyp, der das Format der einer Emailadresse rudimentaer ueberprueft.</xsd:documentation>
    </xsd:annotation>
    
    <xsd:restriction base="xsd:string">
      <xsd:pattern value=".+@.+\..+"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="qb_url">
    <xsd:annotation>
      <xsd:documentation>Datentyp, der das Format einer URL rudimentaer ueberprueft.</xsd:documentation>
    </xsd:annotation>
    
    <xsd:restriction base="xsd:anyURI">
      <xsd:pattern value="((http)|(https))://.+\..+"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="qb_plz">
    <xsd:annotation>
      <xsd:documentation>Datentyp, der das Format der Postleitzahl ueberprueft.</xsd:documentation>
    </xsd:annotation>
    
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="([0-9]){5}"/>
      <xsd:pattern value="CH\-([0-9]){4}"/>
    </xsd:restriction>
  </xsd:simpleType>


