      <xsd:maxLength value="300"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="char_max500">
    <xsd:annotation>
      <xsd:documentation>Datentyp zum Schluesselwort "max. Zeichenanzahl 500" der Datensatzbeschreibung.</xsd:documentation>
    </xsd:annotation>
    
    <xsd:restriction base="xsd:string">
