  <xsd:complexType name="empty">
    <xsd:annotation>
      <xsd:documentation>Datentyp zum Schluesselwort "Kein Inhalt".</xsd:documentation>
    </xsd:annotation>
    
    <xsd:complexContent>
      <xsd:restriction base="xsd:anyType"/>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:simpleType name="char_max100">
    <xsd:annotation>
      <xsd:documentation>Datentyp zum Schluesselwort "max. Zeichenanzahl 100" der Datensatzbeschreibung.</xsd:documentation>
    </xsd:annotation>

    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="100"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="char_max200">
    <xsd:annotation>
      <xsd:documentation>Datentyp zum Schluesselwort "max. Zeichenanzahl 200" der Datensatzbeschreibung.</xsd:documentation>
    </xsd:annotation>
    
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="200"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="char_max300">
    <xsd:annotation>
      <xsd:documentation>Datentyp zum Schluesselwort "max. Zeichenanzahl 200" der Datensatzbeschreibung.</xsd:documentation>
    </xsd:annotation>

    <xsd:restriction base="xsd:string">
