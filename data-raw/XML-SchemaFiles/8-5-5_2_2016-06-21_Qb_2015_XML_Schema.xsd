            <xsd:documentation>Jeder SP Schluessel darf nur einmal angegeben werden.</xsd:documentation>
          </xsd:annotation>
          
          <xsd:selector xpath="Therapeutisches_Personal"/>
          <xsd:field xpath="SP_Schluessel"/>
        </xsd:unique>
        
      </xsd:element>
    </xsd:sequence>
