              </xsd:annotation>
            </xsd:group>

            <xsd:element name="Verantwortlicher_Erstellung">
              <xsd:annotation>
                <xsd:documentation>Fuer die Erstellung des Qualitaetsberichts verantwortliche Person.</xsd:documentation>
              </xsd:annotation>
              
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:group ref="c_Kontakt_Person_lang"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            
            <xsd:element name="Verantwortlicher_Krankenhausleitung">
              <xsd:annotation>
                <xsd:documentation>Fuer die Vollstaendigkeit und Richtigkeit der Angaben im Qualitaetsbericht verantwortliche Person der Krankenhausleitung.</xsd:documentation>
              </xsd:annotation>
              
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:group ref="c_Kontakt_Person_lang"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            
            <xsd:element name="URL_Homepage_Krankenhaus" type="qb_url" minOccurs="0">
