  <xsd:group name="c_Aerztliches_Personal">
    <xsd:annotation>
      <xsd:documentation>Gruppe, die den  Container"Aerztliches_Personal" enthaelt.</xsd:documentation>
    </xsd:annotation>
    
    <xsd:sequence>
      <xsd:element name="Aerztliches_Personal">
        <xsd:annotation>
          <xsd:documentation>Anlage 1: B-X.11.1</xsd:documentation>
        </xsd:annotation>
        
        <xsd:complexType>
          <xsd:choice>
            <xsd:annotation>
              <xsd:documentation>Anlage 1: B-X.1, B X.10.1</xsd:documentation>
              <xsd:documentation>Angabe der Art der Organisationseinheit / Fachabteilung.</xsd:documentation>
            </xsd:annotation>
            
            <xsd:element name="Hauptabteilung">
              <xsd:complexType>
                <xsd:group ref="g_Aerzte_ohne_Belegaerzte"/>
              </xsd:complexType>
            </xsd:element>

            <xsd:element name="Belegabteilung">
              <xsd:complexType>
                <xsd:group ref="c_Belegaerzte"/>
              </xsd:complexType>
            </xsd:element>
            
            <xsd:element name="Gemischte_Haupt_Belegabteilung">
              <xsd:complexType>
                <xsd:group ref="g_Alle_Aerzte"/>
              </xsd:complexType>
            </xsd:element>
            
            <xsd:element name="Nicht_Bettenfuehrend_Abteilung">
              <xsd:complexType>
                <xsd:group ref="g_Alle_Aerzte"/>
              </xsd:complexType>
            </xsd:element>

          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:group>

  <xsd:group name="c_Aerztliche_Fachexpertisen">
