  <xsd:group name="c_Spezielles_Therapeutisches_Personal">
    <xsd:annotation>
      <xsd:documentation>Gruppe, die den Container "Spezielles_Therapeutisches_Personal" enthaelt.</xsd:documentation>
    </xsd:annotation>
    
    <xsd:sequence>
      <xsd:element name="Spezielles_Therapeutisches_Personal">
        <xsd:annotation>
          <xsd:documentation>Anlage 1: A-11.4</xsd:documentation>
        </xsd:annotation>
        
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Therapeutisches_Personal" maxOccurs="unbounded">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="SP_Schluessel" type="enum_Schluessel_SP_ohneSonstige"/>
                  <xsd:sequence>
                    <xsd:group ref="c_Personalerfassung"/>
                  </xsd:sequence>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
        
        <xsd:unique name="sp_schluessel">
