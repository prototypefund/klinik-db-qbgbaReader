      <xsd:documentation>Datentyp zum Wertebereich 0 .. 2 (0 - ja, 1 - nein, 2 - trifft nicht zu)</xsd:documentation>
    </xsd:annotation>

    <xsd:restriction base="xsd:nonNegativeInteger">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="2"/>
    </xsd:restriction>
  </xsd:simpleType>
  
