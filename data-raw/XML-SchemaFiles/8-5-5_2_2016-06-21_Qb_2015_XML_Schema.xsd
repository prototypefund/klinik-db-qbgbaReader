          <xsd:sequence>
            <xsd:element name="Strasse" type="char_max4000"/>
            <xsd:element name="Hausnummer" type="char_max4000"/>
            <xsd:group ref="s_PLZ_Ort"/>
            <xsd:element name="URL_Zugang" type="qb_url" minOccurs="0"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
