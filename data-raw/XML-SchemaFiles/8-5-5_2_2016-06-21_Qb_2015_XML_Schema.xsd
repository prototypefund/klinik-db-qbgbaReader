        <xsd:annotation>
          <xsd:documentation>Anlage 1: B</xsd:documentation>
        </xsd:annotation>
        
        <xsd:complexType>
          <xsd:sequence>
            
            <xsd:element name="Organisationseinheit_Fachabteilung" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Anlage 1: B.X</xsd:documentation>
              </xsd:annotation>
              
              <xsd:complexType>
                <xsd:sequence>
                  
                  <xsd:element name="Gliederungsnummer" type="codomain_1_999_acidtest">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.1</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  
                  <xsd:element name="Name" type="char_max4000">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.1</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>

                  <xsd:group ref="c_Chefaerzte" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.1</xsd:documentation>
                    </xsd:annotation>
                  </xsd:group>

                  <xsd:group ref="c_Zugaenge" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.1</xsd:documentation>
                    </xsd:annotation>
                  </xsd:group>

                  <xsd:group ref="c_Zielvereinbarungen">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.2</xsd:documentation>
                    </xsd:annotation>
                  </xsd:group>
                  
                  <xsd:element name="Ambulante_D_Arzt_Zulassung" type="empty" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.10</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  
                  <xsd:element name="Stationaere_BG_Zulassung" type="empty" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.10</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  
                  <xsd:element name="Fachabteilungsschluessel" maxOccurs="unbounded">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.1</xsd:documentation>
                    </xsd:annotation>
                    
                    <xsd:complexType>
                      <xsd:choice>
                        <xsd:element name="FA_Schluessel" type="qb_Schluessel_FA_ohneSonstige"/>
                        <xsd:element name="Sonstiger">
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element name="FA_Sonstiger_Schluessel" type="enumSchluessel_FA_Sonstige"/>
                              <xsd:group ref="s_Bezeichnung"/>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:choice>
                    </xsd:complexType>
                  </xsd:element>
                  
                  <xsd:group ref="c_Medizinische_Leistungsangebote" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.3</xsd:documentation>
                    </xsd:annotation>
                  </xsd:group>

                  <xsd:group ref="c_Barrierefreiheit" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.4</xsd:documentation>
                    </xsd:annotation>
                  </xsd:group>

                  <xsd:group ref="c_Fallzahlen_OE">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.5</xsd:documentation>
                    </xsd:annotation>
                  </xsd:group>

                  <xsd:choice>
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.6</xsd:documentation>
                    </xsd:annotation>
                    
                    <xsd:group ref="c_Hauptdiagnosen"/>
                    <xsd:element name="Keine_Hauptdiagnose" type="empty"/>
                  </xsd:choice>
                  
                  <xsd:group ref="c_Prozeduren">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.7</xsd:documentation>
                    </xsd:annotation>
                  </xsd:group>
                  
                  <xsd:group ref="c_Ambulante_Behandlungsmoeglichkeiten" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.8</xsd:documentation>
                    </xsd:annotation>
                  </xsd:group>
                  
                  <xsd:group ref="c_Ambulante_Operationen">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.9</xsd:documentation>
                    </xsd:annotation>
                  </xsd:group>
                  
                  <xsd:group ref="c_Personelle_Ausstattung">
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: B-X.11</xsd:documentation>
                    </xsd:annotation>
                  </xsd:group>
                  
                </xsd:sequence>
              </xsd:complexType>
              
              <xsd:unique name="fa_schluessel">
                <xsd:annotation>
                  <xsd:documentation>Jeder Fachabteilungsschluessel mit Ausnahme der Sonstigen Fachabteilungsschluessels darf innerhalb einer Organisationseinheit/Fachabteilung nur einmal angegeben werden.</xsd:documentation>
                </xsd:annotation>
                
                <xsd:selector xpath="Fachabteilungsschluessel"/>
                <xsd:field xpath="FA_Schluessel"/>
              </xsd:unique>
              
            </xsd:element>
            
          </xsd:sequence>
        </xsd:complexType>
        
        <xsd:unique name="oe_nummer">
          <xsd:annotation>
            <xsd:documentation>Die Gliederungsnummer der Organisationseinheit/Fachabteilung muss innerhalb der angegebenen Organisationseinheiten/Fachabteilungen eindeutig sein.</xsd:documentation>
          </xsd:annotation>
          
          <xsd:selector xpath="Organisationseinheit_Fachabteilung"/>
          <xsd:field xpath="Gliederungsnummer"/>
        </xsd:unique>
        
      </xsd:element>
    </xsd:sequence>
    
  </xsd:group>

  
  <xsd:group name="c_QS_nach_Landesrecht">
    <xsd:annotation>
      <xsd:documentation>Gruppe, die den Container "QS_nach_Landesrecht" enthaelt.</xsd:documentation>
    </xsd:annotation>
    
    <xsd:sequence>
      <xsd:element name="QS_nach_Landesrecht">
        <xsd:annotation>
          <xsd:documentation>Anlage 1: C-2</xsd:documentation>
        </xsd:annotation>
        
        <xsd:complexType>
          <xsd:choice>
            <xsd:element name="Teilnahme_landesspezifische_Qualitaetssicherungsmassnahme">
            <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="Leistungsbereich" maxOccurs="4">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="Bezeichnung" type="enum_Leistb_QS_Landesrecht"/>
                        <xsd:element name="Teilnahme_externe_Qualitaetssicherung" type="codomain_0_2"/>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="Keine_Teilnahme_landesspezifische_Qualitaetssicherungsmassnahme" type="empty" />
          </xsd:choice>
        </xsd:complexType>
        
        <xsd:unique name="leistb_qs_landesrecht">
          <xsd:annotation>
            <xsd:documentation>Die Bezeichnung des Leistungsbereiches muss innerhalb der hier angegebenen Leistungsbereiche eindeutig sein.</xsd:documentation>
          </xsd:annotation>
          
          <xsd:selector xpath="Leistungsbereich"/>
          <xsd:field xpath="Bezeichnung"/>
        </xsd:unique>
        
      </xsd:element>
    </xsd:sequence>
  </xsd:group>
  
  <xsd:group name="c_DMP">
    <xsd:annotation>
      <xsd:documentation>Gruppe, die den Container "DMP" enthaelt.</xsd:documentation>
    </xsd:annotation>
    
    <xsd:sequence>
      <xsd:element name="DMP">
        <xsd:annotation>
          <xsd:documentation>Anlage 1: C-3</xsd:documentation>
        </xsd:annotation>
        
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Teilnahme_DMP" maxOccurs="7">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="Bezeichnung" type="enum_DMP"/>
                  <xsd:group ref="s_Erlaeuterungen_500" minOccurs="0"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
        
        <xsd:unique name="dmps">
          <xsd:annotation>
            <xsd:documentation>Die Bezeichnung des Disease-Management-Programm (DMP) muss innerhalb der hier angegebenen teilnehmenden DMPs eindeutig sein.</xsd:documentation>
          </xsd:annotation>
          
          <xsd:selector xpath="Teilnahme_DMP"/>
          <xsd:field xpath="Bezeichnung"/>
        </xsd:unique>
        
      </xsd:element>
    </xsd:sequence>
  </xsd:group>

  <xsd:group name="c_Sonstige_Verfahren_Externe_QS">
    <xsd:annotation>
      <xsd:documentation>Gruppe, die den Container "Sonstige_Verfahren_Externe_QS" enthaelt.</xsd:documentation>
    </xsd:annotation>

    <xsd:sequence>
      <xsd:element name="Sonstige_Verfahren_Externe_QS">
        <xsd:annotation>
          <xsd:documentation>Anlage 1: C-4</xsd:documentation>
        </xsd:annotation>

        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Sonstiges_Verfahren_Externe_QS" maxOccurs="unbounded">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="Leistungsbereich" type="char_max200" minOccurs="0"/>
                  <xsd:element name="Bezeichnung_Qualitaetsindikator" type="char_max200"/>
                  <xsd:element name="Ergebnis" type="char_max100"/>
                  <xsd:element name="Messzeitraum" type="char_max100" minOccurs="0"/>
                  <xsd:element name="Datenerhebung" type="char_max200" minOccurs="0"/>
                  <xsd:element name="Rechenregeln" type="char_max500" minOccurs="0"/>
                  <xsd:element name="Referenzbereiche" type="char_max100" minOccurs="0"/>
                  <xsd:element name="Vergleichswerte" type="char_max100" minOccurs="0"/>
                  <xsd:element name="Quellenangabe_Dokumentation" type="char_max500" minOccurs="0"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>

      </xsd:element>
    </xsd:sequence>
  </xsd:group>

  <xsd:group name="c_Mindestmengen">
    <xsd:annotation>
      <xsd:documentation>Gruppe, die den Container "Mindestmengen" enthaelt.</xsd:documentation>
    </xsd:annotation>
    
    <xsd:sequence>
      <xsd:element name="Mindestmengen">
        <xsd:annotation>
          <xsd:documentation>Anlage 1: C-5</xsd:documentation>
        </xsd:annotation>
        
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Leistungsbereich" maxOccurs="7">
              <xsd:annotation>
                <xsd:documentation>Anlage 1: C-5</xsd:documentation>
              </xsd:annotation>
              
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="Bezeichnung" type="enum_Leistb_Mindestmengen"/>
                  <xsd:element name="Erbrachte_Menge" type="codomain_1_100.000"/>
                  
                  <xsd:choice>
                    <xsd:annotation>
                      <xsd:documentation>Anlage 1: C-5</xsd:documentation>
                    </xsd:annotation>

                    <xsd:element name="Ausnahmetatbestand" maxOccurs="4">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:annotation>
                            <xsd:documentation>Anlage 1: C-5</xsd:documentation>
                          </xsd:annotation>
                          <xsd:element name="MM_Schluessel" type="enum_Schluessel_MM"/>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="Kein_Ausnahmetatbestand" type="empty"/>
                  </xsd:choice>

                  <xsd:group ref="s_Erlaeuterungen_500" minOccurs="0"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
        
        <xsd:unique name="leistb_mindestmengen">
          <xsd:annotation>
            <xsd:documentation>Die Bezeichnung des Leistungsbereiches muss innerhalb der hier angegebenen Leistungsbereiche eindeutig sein.</xsd:documentation>
          </xsd:annotation>
          
          <xsd:selector xpath="Leistungsbereich"/>
          <xsd:field xpath="Bezeichnung"/>
        </xsd:unique>

        <xsd:unique name="mm_schluessel">
          <xsd:annotation>
            <xsd:documentation>Jeder MM Schluessel darf nur einmal angegeben werden.</xsd:documentation>
          </xsd:annotation>

          <xsd:selector xpath="Ausnahmetatbestand"/>
          <xsd:field xpath="MM_Schluessel"/>
        </xsd:unique>

      </xsd:element>
    </xsd:sequence>
  </xsd:group>

  <xsd:group name="c_Strukturqualitaetsvereinbarung">
    <xsd:annotation>
      <xsd:documentation>Gruppe, die den Container "Strukturqualitaetsvereinbarung" enthaelt.</xsd:documentation>
    </xsd:annotation>

    <xsd:sequence>
      <xsd:element name="Strukturqualitaetsvereinbarung">
        <xsd:annotation>
          <xsd:documentation>Anlage 1: C-6</xsd:documentation>
        </xsd:annotation>

        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Vereinbarung" maxOccurs="unbounded">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="CQ_Schluessel" type="enum_Schluessel_CQ"/>
                  <xsd:group ref="s_Erlaeuterungen_500" minOccurs="0"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>

        <xsd:unique name="cq_schluessel">
          <xsd:annotation>
            <xsd:documentation>Jeder CQ Schluessel darf nur einmal angegeben werden.</xsd:documentation>
          </xsd:annotation>

          <xsd:selector xpath="Vereinbarung"/>
          <xsd:field xpath="CQ_Schluessel"/>
        </xsd:unique>

      </xsd:element>
    </xsd:sequence>
  </xsd:group>

  <xsd:group name="c_Fortbildung">
    <xsd:annotation>
      <xsd:documentation>Gruppe, die den Container "Fortbildung" enthaelt.</xsd:documentation>
    </xsd:annotation>

    <xsd:sequence>
      <xsd:element name="Fortbildung">
        <xsd:annotation>
          <xsd:documentation>Anlage 1: C-7</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Fortbildungspflichtige" type="codomain_0_10.000"/>
            <xsd:element name="Nachweispflichtige" type="codomain_0_10.000"/>
            <xsd:element name="Fortbildungsnachweis_Erbracht_Habende" type="codomain_0_10.000"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:group>

  <xsd:group name="c_Qualitaetssicherung">
    <xsd:annotation>
      <xsd:documentation>Gruppe, die den Container "Qualitaetssicherung" enthaelt.</xsd:documentation>
    </xsd:annotation>
    
    <xsd:sequence>
      <xsd:element name="Qualitaetssicherung">
        <xsd:annotation>
          <xsd:documentation>Anlage 1: C</xsd:documentation>
        </xsd:annotation>
        
        <xsd:complexType>
          <xsd:sequence>

            <xsd:group ref="c_QS_nach_Landesrecht">
              <xsd:annotation>
                <xsd:documentation>Anlage 1: C-2</xsd:documentation>
              </xsd:annotation>
            </xsd:group>
            
            <xsd:group ref="c_DMP" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Anlage 1: C-3</xsd:documentation>
              </xsd:annotation>
            </xsd:group>

            <xsd:group ref="c_Sonstige_Verfahren_Externe_QS" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Anlage 1: C-4</xsd:documentation>
              </xsd:annotation>
            </xsd:group>

            <xsd:group ref="c_Mindestmengen" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Anlage 1: C-5</xsd:documentation>
              </xsd:annotation>
            </xsd:group>

            <xsd:group ref="c_Strukturqualitaetsvereinbarung" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Anlage 1: C-6</xsd:documentation>
              </xsd:annotation>
            </xsd:group>

            <xsd:group ref="c_Fortbildung" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Anlage 1: C-7</xsd:documentation>
              </xsd:annotation>
            </xsd:group>

          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    
    </xsd:sequence>
  </xsd:group>
  
  
  <!-- ###########################################################  -->
  <!-- #          globale Deklaration des Rootelementes          #  -->
  <!-- ###########################################################  -->    
  
  <xsd:element name="Qualitaetsbericht">
    <xsd:annotation>
      <xsd:documentation>Rootelement</xsd:documentation>
    </xsd:annotation>
    
    <xsd:complexType>
      <xsd:sequence>
        <xsd:group ref="c_Einleitung"/>

        <xsd:group ref="c_Krankenhaus">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: A-1</xsd:documentation>
          </xsd:annotation>
        </xsd:group>

        <xsd:choice>
          <xsd:element name="Einziger_Standort" type="empty"/>
          <xsd:group ref="c_Standort_dieses_Berichts">
            <xsd:annotation>
              <xsd:documentation>Anlage 1: A-1</xsd:documentation>
            </xsd:annotation>
          </xsd:group>
          <xsd:group ref="c_Standorte_des_Krankenhauses">
            <xsd:annotation>
              <xsd:documentation>Anlage 1: A-1</xsd:documentation>
            </xsd:annotation>
          </xsd:group>
        </xsd:choice>

        <xsd:group ref="c_Krankenhaustraeger">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: A-2</xsd:documentation>
          </xsd:annotation>
        </xsd:group>
                
        <xsd:group ref="c_Akademisches_Lehrkrankenhaus" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: A-3</xsd:documentation>
          </xsd:annotation>
        </xsd:group>
        
        <xsd:group ref="c_Psychiatrisches_Krankenhaus" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: A-4</xsd:documentation>
          </xsd:annotation>
        </xsd:group>
        
        <xsd:element name="Anzahl_Betten" type="codomain_0_20.000">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: A-9</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        
        <xsd:group ref="c_Fallzahlen">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: A-10</xsd:documentation>
          </xsd:annotation>
        </xsd:group>

        <xsd:group ref="c_Medizinisch_Pflegerische_Leistungsangebote" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: A-5</xsd:documentation>
          </xsd:annotation>
        </xsd:group>

        <xsd:group ref="c_Nicht_Medizinische_Leistungsangebote" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: A-6</xsd:documentation>
          </xsd:annotation>
        </xsd:group>

        <xsd:group ref="c_Barrierefreiheit" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: A-7</xsd:documentation>
          </xsd:annotation>
        </xsd:group>

        <xsd:group ref="c_Akademische_Lehre" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: A-8.1</xsd:documentation>
          </xsd:annotation>
        </xsd:group>

        <xsd:group ref="c_Ausbildung_andere_Heilberufe" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: A-8.2</xsd:documentation>
          </xsd:annotation>
        </xsd:group>

        <xsd:group ref="c_Personal_des_Krankenhauses">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: A-11</xsd:documentation>
          </xsd:annotation>
        </xsd:group>

        <xsd:group ref="c_Umgang_mit_Risiken_in_der_Patientenversorgung">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: A-12</xsd:documentation>
          </xsd:annotation>
        </xsd:group>

        <xsd:group ref="c_Apparative_Ausstattung" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: A-13</xsd:documentation>
          </xsd:annotation>
        </xsd:group>

        <xsd:group ref="c_Organisationseinheiten_Fachabteilungen">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: B</xsd:documentation>
          </xsd:annotation>
        </xsd:group>
        
        <xsd:group ref="c_Qualitaetssicherung">
          <xsd:annotation>
            <xsd:documentation>Anlage 1: C</xsd:documentation>
          </xsd:annotation>
        </xsd:group>
        
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  
</xsd:schema>