                <xsd:documentation>Anlage 1: B-X.11.2</xsd:documentation>
              </xsd:annotation>
            </xsd:group>

            <xsd:group ref="c_Ausgewaehltes_Therapeutisches_Personal_Psycho" minOccurs="0">
